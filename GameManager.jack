class GameManager{
    static int screen_left, screen_right, screen_top, screen_bottom, infection_radius, infection_prob, num_of_people, num_vaxed, num_infected, num_dead;
    static int graph1_start_x, graph1_start_y, graph1_length, graph1_width;
    static int graph2_start_x, graph2_start_y, graph2_length, graph2_width;

    

    function void input_screen(){
        do Screen.clearScreen();
        do Sys.wait(300);
        do GameManager.draw_title(1 + (32*10));
        do GameManager.draw_bat_soup(24 + (32*100));
        do Output.moveCursor(22, 40);
        do Output.printString("(c) Kelner & Kelner 2021");
        let num_of_people = GameManager.get_user_input(7, "Enter num of people (max 20): ", 1);
        let num_vaxed = GameManager.get_user_input(9, "How many of them are vaccinated?: ", 2);
        let num_infected = GameManager.get_user_input(11, "How many of them are infected? (min 1): ", 3);
        let infection_prob = ((-9)*GameManager.get_user_input(13, "Enter the infection factor (1-10): ", 4))+92;

        do Screen.clearScreen();
        
        return;
    }
    

    function void start_game () {
        var Population pop;
        var int x;
        var boolean running;
        
        let screen_left = 16;
        let screen_right = 400;
        let screen_top = 40;
        let screen_bottom = 240;
        let infection_radius = 23;

        let graph1_start_x = 420;
        let graph1_start_y = 100;
        let graph1_length = 140;
        let graph1_width = 25;

        let graph2_start_x = 460;
        let graph2_start_y = 100;
        let graph2_length = 140;
        let graph2_width = 25;

        let num_dead = 0;

        
        do RandGen.set_seed(num_of_people+num_vaxed+num_infected+infection_prob);
        
        let pop = Population.new(num_of_people, num_infected, num_vaxed);
        do GameManager.write_num_dead();
        do GameManager.write_num_alive();
        do GameManager.write_num_infected();
        do GameManager.init_graph1();
        do GameManager.init_graph2();
        do GameManager.init_borders();
        do GameManager.draw_legends();
        do GameManager.draw_virus_logo(26 + (32*23));


        let running = true;
        while (running){
            do pop.movePeople();



            if (~(num_dead=pop.get_num_dead())){
                let num_dead = pop.get_num_dead();
                do GameManager.write_num_dead();
                do GameManager.write_num_alive();
                do GameManager.draw_graph1();
                do GameManager.draw_graph2();
            }

            if (~(num_infected=pop.get_num_infected())){
                let num_infected = pop.get_num_infected();
                do GameManager.write_num_infected();
                do GameManager.draw_graph2();
                if (num_infected = 0){
                    let running = false;
                }
            }

        }
        do pop.movePeople();
        do Sys.wait(1000);
        do pop.dispose();
        return;
    }

    function int get_user_input(int row, String message, int field_num){
        var boolean valid, check;
        var String buffer;
        let valid = false;
        do Output.moveCursor(row,5);
        let buffer = Keyboard.readLine(message);
        //do Output.printInt(String.length(buffer));
        while(~(valid)){
            if (field_num = 1){
                let check = GameManager.is_valid_people(buffer);
            }
            if (field_num = 2){
                let check = GameManager.is_valid_vaxed(buffer);
            }
            if (field_num = 3){
                let check = GameManager.is_valid_infected(buffer);
            }
            if (field_num = 4){
                let check = GameManager.is_valid_factor(buffer);
            }

            if (check){
                let valid = true;
            }
                
            else{
                do GameManager.invalid_feedback();
                do Output.moveCursor(row,5);
                do Output.printString(message);
                do Output.printString(buffer);
                do GameManager.delete_n(String.length(buffer));
                let buffer = Keyboard.readLine("");
            }          
        }
        return String.intValue(buffer);
    }


    function boolean end_game(){
        do Output.moveCursor(9,14);
        do Output.printString("CONGRATSULATIONS! ");
        do Sys.wait(1000);
        do Output.moveCursor(10,12);
        do Output.printString("YOU HAVE BEATEN THE DISEASE!");
        do Sys.wait(2000);
        do Output.moveCursor(11,14);
        do Output.printString("BUT... ");
        do Sys.wait(1000);
        do Output.printString("AT WHAT COST??");
        do Output.moveCursor(12,12);
        do Sys.wait(2000);
        do Output.printInt(num_dead);
        do Output.printString(" OF THE ORIGINAL "); 
        do Output.printInt(num_of_people);
        do Output.printString(" PEOPLE"); 
        do Output.moveCursor(13,10);
        do Output.printString(" DID NOT MAKE IT TO THE END..."); 
        do Sys.wait(1000);
        return GameManager.restart_question();
    }

    function boolean restart_question(){
        var char key;
        var boolean done;
        do Output.moveCursor(15,15);
        do Output.printString("RESTART");
        do Output.moveCursor(15,30);
        do Output.printString("<R>");

        do Output.moveCursor(16,15);
        do Output.printString("QUIT");
        do Output.moveCursor(16,30);
        do Output.printString("<Q>");

        
        let done = false;
        while (~done){
            let key = Keyboard.keyPressed();
            if ((key=82)|(key = 81)){
                let done = true;
            }
        }

        if (key = 82){
            return true;
        }
        return false;
    }

