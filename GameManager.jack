    function void init_borders(){
        do Screen.setColor(true);
        do Screen.drawRectangle(screen_left-3,screen_top-3,screen_left, screen_bottom+3);
        do Screen.drawRectangle(screen_left-3,screen_top-3,screen_right+3, screen_top);
        do Screen.drawRectangle(screen_right,screen_top-3,screen_right+3, screen_bottom+3);
        do Screen.drawRectangle(screen_left-3,screen_bottom,screen_right+3, screen_bottom+3);
        return;
    }

    function void init_graph1(){
        do Screen.setColor(true);
        do Screen.drawRectangle(graph1_start_x,graph1_start_y,graph1_start_x + graph1_width,graph1_start_y + graph1_length);
        
        do Screen.setColor(false);
        do Screen.drawRectangle(graph1_start_x +2 ,graph1_start_y +2,graph1_start_x + graph1_width -2,graph1_start_y + graph1_length -2);
        do GameManager.draw_graph1();
        return;
    }


    function void init_graph2(){
        do Screen.setColor(true);
        do Screen.drawRectangle(graph2_start_x,graph2_start_y,graph2_start_x + graph2_width,graph2_start_y + graph2_length);
        
        do Screen.setColor(false);
        do Screen.drawRectangle(graph2_start_x +2 ,graph2_start_y +2,graph2_start_x + graph2_width -2,graph2_start_y + graph2_length -2);
        do GameManager.draw_graph2();
        return;
    }

    
    function void draw_graph1(){
        var int lable_col, label_row;
        var int length;
        let label_row= 12;
        let lable_col = 54;
        let length = graph1_length * num_dead / num_of_people;
        do Screen.setColor(true);
        do Screen.drawRectangle(graph1_start_x,graph1_start_y,graph1_start_x + graph1_width,graph1_start_y + length);

        do GameManager.write_vertically(label_row, lable_col, "ALIVE");

        return;
    }

    function void draw_graph2(){
        var int length;
        var int lable_col, label_row;
        let label_row= 12;
        let lable_col = 59;
        let length = Math.min(graph2_length * num_infected / (num_of_people-num_dead), graph2_length);
        do Screen.setColor(true);
        do Screen.drawRectangle(graph2_start_x,graph2_start_y,graph2_start_x + graph2_width,graph2_start_y + length);
        if (num_of_people-num_dead -num_infected > 0 ){
            do Screen.setColor(false);
            do Screen.drawRectangle(graph2_start_x +2,graph2_start_y + length+2, graph2_start_x+ graph2_width -2,graph2_start_y+graph2_length -2);
        }

        do GameManager.write_vertically(label_row, lable_col, "HEALTHY");
        return;
    }

    function void write_vertically(int row, int col, String text){
        var int i, len;
        let len= text.length();
        let i=0;
        do Screen.setColor(true);
        while(i<len){
            do Output.moveCursor(row+i,col);
            do Output.printChar(text.charAt(i));
            let i = i+1;
        }

        return;
        
    }



    function void write_num_alive(){
        do Output.moveCursor(1,1);
        do Output.printString("ALIVE");
        do Output.moveCursor(2,5);
        do GameManager.delete_n(5);
        do Output.moveCursor(2,3);
        do Output.printInt(num_of_people - num_dead);
        return;
    }

