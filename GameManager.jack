    function void draw_legends(){
        var Person unvaxed, vaxed, infected, recovered;
        do Output.moveCursor(0,22);
        do Output.printString("UNVAXED");
        let unvaxed = Person.new(200, screen_top - 17, 0, 0, false, false);
        do Output.moveCursor(0,30);
        do Output.printString("VAXED");
        let vaxed = Person.new(255, screen_top - 17, 0, 0, false, true);
        do Output.moveCursor(0,36);
        do Output.printString("INFECTED");
        let infected = Person.new(317, screen_top - 17, 0, 0, true, false);
        
        do Output.moveCursor(0,45);
        do Output.printString("RECOVERED");
        let recovered = Person.new(390, screen_top - 17, 0, 0, true, false);
        do recovered.draw_recovered();


        do unvaxed.dispose();
        do vaxed.dispose();
        do infected.dispose();
        do recovered.dispose();
        return;
        
    }

    function void delete_n(int n){
        var int i;
        let i =0;
        while (i<n){
            do Output.backSpace();
            let i = i+1;
        }
        return;
    }

    function boolean is_valid_people (String buffer) {
        var int n;
        let n = String.intValue(buffer);
        if ((n<1)|(n>20)){
            return false;
        }
        return true;
    }


    function boolean is_valid_vaxed (String buffer) {
        var int n;
        let n = String.intValue(buffer);
        

        if ((n<0)|(n>(num_of_people-1))){
            return false;
        }
        return true;
    }

    function boolean is_valid_infected (String buffer) {
        var int n;
        let n = String.intValue(buffer);
        if ((n<1)|(n>(num_of_people-num_vaxed))){
            return false;
        }
        return true;
    }


        
    function boolean is_valid_factor (String buffer) {
        var int n;
        let n = String.intValue(buffer);
        if ((n<0)|(n>10)){
            return false;
        }
        return true;
    }

    function void invalid_feedback(){
        var int x,y, size;
        let x = 350;
        let y = 70;
        let size = 20;
        do Screen.setColor(true);
        do Screen.drawLine(x,y,x+size, y+size);
        do Screen.drawLine(x+size,y,x, y+size);
        do Sys.wait(500);
        do Screen.setColor(false);
        do Screen.drawLine(x,y,x+size, y+size);
        do Screen.drawLine(x+size,y,x, y+size);
        return;
    }

